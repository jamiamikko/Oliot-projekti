	/**
	 * Piirretaan nuoli vasemmalle
	 */

	public static void DrawArrowLeft() {
		gLCD.clear();
		gLCD.drawImage(leftArrow, 40, 20, 0);

	}

	/**
	 * Piirretaan nuoli oikealle
	 */
	
	public static void DrawArrowRight() {
		gLCD.clear();
		gLCD.drawImage(rightArrow, 40, 20, 0);
	}
	
	/**
	 * Piirretaan nuoli eteen
	 */

	public static void DrawArrowForward() {
		gLCD.clear();
		gLCD.drawImage(forwardArrow, 40, 20, 0);
	}

	/**
	 * Piirretaan nuoli taakse
	 */
	
	public static void DrawArrowReverse() {
		gLCD.clear();
		gLCD.drawImage(reverseArrow, 40, 20, 0);
	}
	

}
